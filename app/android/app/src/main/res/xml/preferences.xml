<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
  <ListPreference android:title="@string/prefs_threshold_title"
                  android:summary="@string/prefs_threshold_summary"
                  android:key="riskThreshold"
                  android:defaultValue="0.6"
                  android:entries="@array/listLabelsRisk"
                  android:entryValues="@array/listValuesRisk" />
  <ListPreference android:title="Data-entry form"
      android:summary="Select the form to enter patient data"
      android:key="dataEntryForm"
      android:defaultValue="0"
      android:entries="@array/listLabelsForm"
      android:entryValues="@array/listValuesForm" />
</PreferenceScreen>